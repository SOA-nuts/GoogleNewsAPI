.container
  .row
    h2 id='sub_header'
      / - company = "Company: #{target.company_name}"
      span.target.name
        = target.company_name
      / span.target.name
      /   = histories.company
  .container


    table class="table table-hover" id="score_table"
        thead
          tr
            th Article
            th Score
        tbody
          - target.articles.each do |article, index|
            tr
              td = article.title
              td = article.score